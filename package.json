{
  "name": "greact",
  "version": "1.0.5",
  "description": "A grid system for React",
  "main": "./lib/greact.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run clean && npm run build:babel && npm run build:webpack",
    "build:babel": "babel -d lib/ src/",
    "build:demo": "cross-env webpack -p --config webpack.config.demo.js",
    "build:webpack": "cross-env webpack -p --config webpack.config.prod.js",
    "clean": "rimraf lib && rimraf umd",
    "start": "node demo/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kncs/greact.git"
  },
  "keywords": [
    "Grid",
    "Layout",
    "React",
    "Row",
    "Col",
    "react-component"
  ],
  "author": "Kevin Clarens",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kncs/greact/issues"
  },
  "homepage": "https://github.com/kncs/greact#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "cross-env": "^1.0.7",
    "express": "^4.13.4",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "rimraf": "^2.5.2",
    "webpack": "^1.13.3"
  },
  "peerDependencies": {
    "react": ">=0.15.0"
  },
  "dependencies": {
    "react-responsive": "^1.2.1"
  }
}
